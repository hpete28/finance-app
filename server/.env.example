AI_SHARE_AMOUNT=false
OLLAMA_MAX_BATCH=80
OLLAMA_BASE_URL=http://127.0.0.1:11434
OLLAMA_MODEL=llama3.1:8b
OLLAMA_TIMEOUT_MS=45000
OLLAMA_KEEP_ALIVE=15m
OLLAMA_MIN_CATEGORY_CONFIDENCE=0.72
